package main

import (
	"fmt"
	"sync"
)

type ccc map[string]int

func main() {
	var aM sync.Mutex
	cc := make(ccc)
	cc["hh"] = 1
	cc["ss"] = 2
	cc["nn"] = 3

	fmt.Println(cc)

	de(cc, "ss", aM)

	fmt.Println(cc)
}

func de(a map[string]int, b string, lock sync.Mutex) {
	lock.Lock()
	delete(a, b)
	lock.Unlock()
}
